<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="270" android:endColor="@color/highlighted_text_material_light" android:startColor="@color/highlighted_text_material_light" />
            <stroke android:width="1dp" android:color="@color/highlighted_text_material_light" />
            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="@color/material_deep_teal_500" android:startColor="@color/material_deep_teal_500" />
            <stroke android:width="1dp" android:color="@color/material_deep_teal_500" />
            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape>
    </item>
</selector>